{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "NUXT_APP_BASE_URL=/ nuxt dev --host",
    "clean:dsstore": "find . -name '.DS_Store' -type f -delete",
    "generate": "npm run clean:dsstore && NUXT_APP_BASE_URL=/ nuxt generate",
    "generateGHPages": "npm run clean:dsstore && NUXT_APP_BASE_URL=/gfx_snapshot_visualisation/ nuxt generate",
    "generateLocal": "NUXT_APP_BASE_URL=/taserbeam/ nuxt generate --no-clean",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "nuxt": "^3.14.1592",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "sass": "^1.82.0"
  }
}
